{
  "name": "mvo/contao-nested-forms",
  "type": "contao-bundle",
  "description": "Allow nested forms/sub forms in Contao Open Source CMS",
  "keywords": [
    "contao",
    "forms",
    "nested",
    "sub",
    "embed",
    "form generator"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Moritz Vondano",
      "homepage": "https://github.com/m-vo/contao-nested-forms"
    }
  ],
  "require": {
    "php": "^7.4 || ^8.0",
    "contao/core-bundle": "^4.9",
    "doctrine/dbal": "^3.3"
  },
  "require-dev": {
    "contao/manager-plugin": "^2.0",
    "friendsofphp/php-cs-fixer": "^2.2"
  },
  "autoload": {
    "psr-4": {
      "Mvo\\ContaoNestedForms\\": "src/"
    }
  },
  "extra": {
    "contao-manager-plugin": "Mvo\\ContaoNestedForms\\ContaoManager\\Plugin"
  },
  "config": {
    "allow-plugins": {
      "contao-components/installer": true,
      "ocramius/package-versions": true,
      "contao/manager-plugin": true
    }
  }
}
